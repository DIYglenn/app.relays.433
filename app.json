{
  "id": "app.relays.433",
  "name": {
    "en": "433 Relays App"
  },
  "version": "0.0.1",
  "sdk": 2,
  "compatibility": ">=1.5",
  "category": "appliances",
  "description": {
    "en": "Let Homey control your 433 MHz relays"
  },
  "images": {
    "large": "./assets/images/large.png",
    "small": "./assets/images/small.png"
  },
  "author": {
    "name": "Glenn Jacobsen",
    "email": "info@athom.com"
  },
  "signals": {
    "433": {
      "brel": {
	      "sof": [],
        "eof": [
          465,
          3859
        ],
        "words": [
          [
            1396,
            438
          ],
          [
            463,
            1304
          ]
        ],
        "interval": 10000,
        "repetitions": 20,
        "sensitivity": 0.5,
        "minimalLength": 24,
        "maximalLength": 24
      }
    }
  },
  "drivers": [
    {
      "id": "MLE-25",
      "name": {
        "en": "Brel Remote"
      },
      "icon": "",
      "class": "windowcoverings",
      "capabilities": [
        "windowcoverings_state"
      ],
      "images": {
        "large": "./drivers/MLE-25/assets/images/large.jpg",
        "small": "./drivers/MLE-25/assets/images/small.jpg"
      },
      "rf": {
        "signal": "brel"
      },
      "settings": [],
      "pair": [
        {
          "id": "imitate",
          "rf_template": "imitate",
          "navigation": {}
        },
        {
          "id": "test_switch",
          "rf_template": "test_switch",
          "navigation": {
            "prev": "imitate",
            "next": "done"
          }
        },
        {
          "id": "done",
          "rf_template": "done"
        }
      ]
    }
  ],
  "permissions": [
    "homey:wireless:433"
  ]
}
